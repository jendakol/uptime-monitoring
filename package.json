{
    "name": "google-cloud-functions-typescript",
    "version": "0.0.4",
    "description": "This is a TypeScript based Cloud Functions template.",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf \"functions/*\"",
        "lint": "tslint --project tsconfig.json",
        "tsc": "tsc",
        "cp-package": "copyfiles package.json functions/src",
        "build": "run-s clean lint tsc cp-package",
        "test": "mocha \"functions/test/\"",
        "deploy": "rm -rf functions && npm run build && cd functions/src && gcloud functions deploy --runtime nodejs16 --trigger-http --project jendapersonal"
    },
    "bin": {
        "create-project": "./cli.js"
    },
    "keywords": [
        "cloud",
        "functions",
        "typescript"
    ],
    "author": "\"@flatfisher <kyoheitsuda@flatfish.jp> (https://kyohei.flatfish.jp)",
    "license": "MIT",
    "dependencies": {
        "google-cloud-functions-typescript": "^0.0.4"
    },
    "devDependencies": {
        "@types/chai": "4.2.22",
        "@types/chai-http": "4.2.0",
        "@types/express": "4.17.13",
        "@types/mocha": "9.0.0",
        "@types/moment": "2.13.0",
        "@types/node": "16.11.10",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "copyfiles": "2.4.1",
        "express": "4.17.1",
        "mocha": "9.1.3",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "tslint": "6.1.3",
        "typescript": "4.5.2"
    }
}
